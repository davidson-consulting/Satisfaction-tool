<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Satisfaction tool!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet" href="{{ asset('bundles/satisfaction/css/jquery-ui.css')}}" />
        <link rel="stylesheet" href="{{ asset('bundles/satisfaction/css/app.css')}}" />
    {% endblock %}

</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}
    <script type="text/javascript" src="{{ asset('bundles/satisfaction/js/jquery-1.10.2.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset ('bundles/satisfaction/js/jquery-2.0.js')}}" ></script>
    <script type="text/javascript" src="{{ asset('bundles/satisfaction/js/jquery-ui.js')}}" ></script>
    <script type="text/javascript" src="{{ asset('bundles/satisfaction/js/jquery-form-min.js')}}" ></script>
    <script type="text/javascript" src="{{ asset ('bundles/satisfaction/js/app.js')}}" ></script>
{% endblock %}
</body>
</html>
